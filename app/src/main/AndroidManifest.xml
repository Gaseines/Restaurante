<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    package="br.com.gsn.restaurante">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Restaurante"
        tools:targetApi="31">
        <activity
            android:name=".Cartao.MainCartao"
            android:exported="false" />
        <activity
            android:name=".Endereco.MainEndereco"
            android:exported="false" />
        <activity
            android:name=".Carrinho.MainCarrinho"
            android:exported="false" />
        <activity
            android:name=".MainPerfil"
            android:exported="false" />
        <activity
            android:name=".Cardapio.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>